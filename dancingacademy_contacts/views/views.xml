<odoo>

<record id="dancingacademy_all_members_views" model="ir.ui.view">
            <field name="name">dancingacademy.all.members.views</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_type']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <!-- <xpath expr="//field[@name='company_type']" position="after">
                    <field name="type_member" options="{'horizontal': true}" widget="radio"/>
                </xpath> -->
                <xpath expr="//field[@name='parent_id']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//div[@class='o_address_format']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='vat']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='function']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='website']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='title']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='active_lang_count']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='lang']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
                <xpath expr="//field[@name='category_id']" position="attributes">
                    <attribute name="attrs">{'invisible': 1}</attribute>
                </xpath>
            </field>
    </record>

    <record id="contacts.menu_contacts" model="ir.ui.menu">
        <field name="name">Members</field>
    </record>

    <record id="contacts.res_partner_menu_contacts" model="ir.ui.menu">
        <field name="name">All Members</field>
    </record>


</odoo>
    
